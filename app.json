{
  "expo": {
    "name": "foolery-the-game",
    "slug": "foolery-the-game",
    "version": "1.0.0",
    "newArchEnabled": true,
    "orientation": "portrait",
    "sdkVersion": "52.0.0",
    "icon": "./assets/images/logo.webp",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Baloo-VariableFont_wght",
            "./assets/fonts/Quicksand-VariableFont_wght"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
